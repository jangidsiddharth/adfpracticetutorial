{
	"name": "SCDtype1",
	"properties": {
		"description": "Slowly Dimension Change table. ",
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "EmpBlobSource",
						"type": "DatasetReference"
					},
					"name": "EmpBlobSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "EmpSqlTable",
						"type": "DatasetReference"
					},
					"name": "EmpSqlSink"
				}
			],
			"transformations": [
				{
					"name": "alterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empId as integer,",
				"          ename as string,",
				"          email as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EmpBlobSource",
				"EmpBlobSource alterRow(upsertIf(1==1)) ~> alterRow1",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          empId as integer,",
				"          ename as string,",
				"          email as string",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:false,",
				"     upsertable:true,",
				"     keys:['empId'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          empId,",
				"          ename,",
				"          email",
				"     )) ~> EmpSqlSink"
			]
		}
	}
}